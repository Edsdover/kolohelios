.row(ng-show='showProjectView')
  .col-xs-12.center
    h3 {{projects[view].name}}
    h4 Case Study
  .col-xs-6.center
    img(ng-src='{{projects[view].screenshot}}')
  .col-xs-6.center
    p {{projects[view].extddesc}}
    p Frameworks:
    ul
      li(ng-repeat='tech in projects[view].techs') {{tech}}
    p Role: {{projects[view].role}}
    a(ng-href='{{projects[view].linktoapp}}') {{projects[view].linktext}}
